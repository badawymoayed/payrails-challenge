{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AlphaVantage Time Series Weekly Schema",
  "type": "object",
  "required": [
    "Meta Data",
    "Weekly Time Series"
  ],
  "properties": {
    "Meta Data": {
      "type": "object",
      "required": [
        "1. Information",
        "2. Symbol",
        "3. Last Refreshed",
        "4. Time Zone"
      ]
    },
    "Weekly Time Series": {
      "type": "object",
      "patternProperties": {
        "^[0-9]{4}-[0-9]{2}-[0-9]{2}$": {
          "type": "object",
          "required": [
            "1. open",
            "2. high",
            "3. low",
            "4. close",
            "5. volume"
          ]
        }
      }
    }
  }
}