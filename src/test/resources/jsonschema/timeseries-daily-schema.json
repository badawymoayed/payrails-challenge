{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AlphaVantage Time Series Daily Schema",
  "type": "object",
  "required": [
    "Meta Data",
    "Time Series (Daily)"
  ],
  "properties": {
    "Meta Data": {
      "type": "object",
      "required": [
        "1. Information",
        "2. Symbol",
        "3. Last Refreshed",
        "4. Output Size",
        "5. Time Zone"
      ],
      "properties": {
        "1. Information": { "type": "string" },
        "2. Symbol": { "type": "string" },
        "3. Last Refreshed": { "type": "string" },
        "4. Output Size": { "type": "string" },
        "5. Time Zone": { "type": "string" }
      }
    },
    "Time Series (Daily)": {
      "type": "object",
      "patternProperties": {
        "^[0-9]{4}-[0-9]{2}-[0-9]{2}$": {
          "type": "object",
          "required": [
            "1. open",
            "2. high",
            "3. low",
            "4. close",
            "5. volume"
          ],
          "properties": {
            "1. open": { "type": "string" },
            "2. high": { "type": "string" },
            "3. low": { "type": "string" },
            "4. close": { "type": "string" },
            "5. volume": { "type": "string" }
          }
        }
      }
    }
  }
}